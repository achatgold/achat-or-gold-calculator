{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors https://*.wixsite.com https://www.achatormontreal.com https://achatormontreal.com"
        }
      ]
    }
  ]
}
